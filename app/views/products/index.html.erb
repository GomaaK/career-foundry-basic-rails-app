  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <h1>Browse Bikes</h1>
      </div><!-- end col -->
    </div><!-- end row -->  
  </div><!-- end container -->

  <div class="container">  
    <div class="row">   
      <% @products.each do |product| %>  
        <div class="col-xs-12 col-md-4">
          <a href="#" class="product">
            <%= image_tag(product.image_url, class: "img-responsive") %>
          </a>
          <h3><%= product.name %></h3>
          <p><%= product.description %></p>
          <p><%= product.price %></p>  
        </div><!-- end thumbnail image -->
      <% end %><!-- end product loop -->
    </div><!-- end row -->
  </div><!--  end content container -->